{
  "name": "expand-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && build-storybook -o build/storybook -s ./build/storybook/,./docs/,./public",
    "build:qa": "env-cmd -f .env.qa react-scripts build && build-storybook -o build/storybook -s ./build/storybook/,./docs/,./public",
    "build:staging": "env-cmd -f .env.staging react-scripts build && build-storybook -o build/storybook -s ./build/storybook/,./docs/,./public",
    "build:prod": "env-cmd -f .env.production react-scripts build && build-storybook -o build/storybook -s ./build/storybook/,./docs/,./public",
    "set-permissions": "node scripts/set_policy.js",
    "test": "react-scripts test --watchAll=false --updateSnapshot --forceExit --detectOpenHandles",
    "eject": "react-scripts eject",
    "lint": "prettier --write ./**/*.{js,jsx,ts,tsx}",
    "cp": "./node_modules/create-react-component-folder/lib/index.js -f --typescript --nocss --stories -ns -o src/components",
    "tsc": "tsc --noEmit --skipLibCheck --project tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.2.0",
    "@okta/okta-auth-js": "^4.0.0",
    "@okta/okta-react": "^3.0.7",
    "@okta/okta-signin-widget": "^4.4.3",
    "@sentry/react": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/md5": "^2.3.1",
    "@types/react-autosuggest": "^10.0.1",
    "@types/react-avatar-editor": "^12.0.0",
    "@types/uuid": "^8.3.0",
    "apollo-link-sentry": "^3.2.0",
    "classnames": "^2.3.1",
    "country-code-emoji": "^1.0.0",
    "decimal.js-light": "^2.5.1",
    "env-cmd": "^10.1.0",
    "framer-motion": "^2.6.13",
    "graphql": "^15.3.0",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.0.0",
    "md5": "^2.3.0",
    "moment": "^2.28.0",
    "next": "9.3.5",
    "pdfjs-dist": "^2.7.570",
    "react": "16.13.1",
    "react-autosuggest": "^10.1.0",
    "react-avatar-editor": "^12.0.0",
    "react-d3-speedometer": "^1.0.2",
    "react-dom": "16.13.1",
    "react-idle-timer": "^4.6.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-swipeable-list": "^1.2.0",
    "react-tiny-popover": "^6.0.2",
    "recharts": "^2.1.10",
    "serverless-s3-sync": "^1.14.4",
    "theme-ui": "^0.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.21",
    "@types/enzyme": "^3.10.6",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.10.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/theme-ui": "^0.3.6",
    "create-react-component-folder": "^0.3.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "husky": "^4.3.0",
    "react-is": "^16.13.1",
    "serverless-s3-encryption": "^1.0.0",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test && yarn tsc"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
